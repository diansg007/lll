#include <iostream>
#include <windows.h>
using namespace std;
int main() {
	SetConsoleCP(1252);
	SetConsoleOutputCP(1251);
	int n;
	cout << "Введіть ціле позитивне число: ";
	cin >> n;
	bool result = (n >= 10 && n <= 99 && n % 2 == 0);
	cout << "Число є парним двозначним? " << boolalpha << result << endl;
	system("pause");
	return 0;
}
